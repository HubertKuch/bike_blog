{
    "name": "hubert/bike-blog",
    "type": "project",
    "autoload": {
        "classmap": [
          "Configuration",
          "Controllers",
          "Exceptions",
          "Models",
          "Resources",
          "Tests",
          "BikeApplication.php",
          "Helpers",
          "Logger",
          "Services"
        ],
        "psr-4": {
          "Hubert\\BikeBlog\\Configuration\\": "Configuration",
          "Hubert\\BikeBlog\\Models\\": "Models",
          "Hubert\\BikeBlog\\Exceptions\\": "Exceptions",
          "Hubert\\BikeBlog\\Controllers\\": "Controllers",
          "Hubert\\BikeBlog\\Resources\\": "Resources",
          "Hubert\\BikeBlog\\Tests\\": "Tests",
          "Hubert\\BikeBlog\\Utils\\": "Utils",
          "Hubert\\BikeBlog\\Client\\": "Client",
          "Hubert\\BikeBlog\\Helpers\\": "Helpers",
          "Hubert\\BikeBlog\\Logger\\": "Logger",
          "Hubert\\BikeBlog\\Services\\": "Services"
        }
    },
    "authors": [
        {
          "name": "hubert",
          "email": "kuchhubert@gmail.com"
        }
    ],
  "minimum-stability": "dev",
  "require": {
    "vlucas/phpdotenv": "v5.0.0",
    "ramsey/uuid": "4.4.0",
    "nesbot/carbon": "2.62.0",
    "salesforce/handlebars-php": "2.3.0",
    "hubert/avocado": "v5.2.0",
    "ext-pdo": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "7.5.0",
    "phpunit/phpunit": "9.5.24"
  },
  "config": {
    "allow-plugins": {
      "nodejs-php-fallback/nodejs-php-fallback": true
    }
  },
  "scripts": {
    "test": "phpunit ./Tests/Unit/"
  }
}
